				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LotNumber" type="LotNumber" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="RxFillIndicator" type="RxFillIndicator" minOccurs="0" maxOccurs="0"/>
