	<xsd:complexType name="NumericChoiceForPARequest">
		<xsd:complexContent>
			<xsd:extension base="NumericTypeBasic">
				<xsd:sequence>
					<xsd:element name="Answer" type="CompletedAnswerChoiceForNumericType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="NumericType">
		<xsd:choice>
			<xsd:element name="SingleComparison" type="NumericValueSimpleType"/>
			<xsd:element name="RangeComparison" type="NumericValueType"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="NumericTypeBasic">
		<xsd:sequence>
			<xsd:element name="IsNumeric" type="BooleanCode" default="Y">
				<xsd:annotation>
					<xsd:documentation>A value of "Y" to indicate the answer to the question is Numeric. A value of "N" should not be used. Use other question type if not Numeric.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NextQuestionCondition" type="NumericType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="NumericValueSimpleType">
		<xsd:sequence>
			<xsd:element name="ComparisonOperator" type="ComparisonOperator">
				<xsd:annotation>
					<xsd:documentation>Code that conveys the relationship between the answered value to a question and a defined boundary.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ComparisonValue" type="sn1..18">
				<xsd:annotation>
					<xsd:documentation>Value for the boundary of the comparison.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NextQuestionID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>A QuestionID or value of "END" to identify the next question when it is dependent on the answer to the question (i.e., answer choice or numeric condition). Required when the DefaultNextQuestionID for the question is not provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="NumericValueType">
		<xsd:sequence>
			<xsd:element name="LowerBoundComparisonValue" type="sn1..18">
				<xsd:annotation>
					<xsd:documentation>Value for the lower boundary of the comparison.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LowerBoundComparisonOperator" type="LowerBoundComparisonOperator">
				<xsd:annotation>
					<xsd:documentation>Code that conveys the relationship between the answered value to a question and a defined lower boundary</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UpperBoundComparisonValue" type="sn1..18">
				<xsd:annotation>
					<xsd:documentation>Value for the upper boundary of the comparison.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UpperBoundComparisonOperator" type="UpperBoundComparisonOperator">
				<xsd:annotation>
					<xsd:documentation>Code that conveys the relationship between the answered value to a question and a defined upper boundary.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NextQuestionID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>A QuestionID or value of "END" to identify the next question when it is dependent on the answer to the question (i.e., answer choice or numeric condition). Required when the DefaultNextQuestionID for the question is not provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Observation">
		<xsd:annotation>
			<xsd:documentation>Provider validated height and weight is required when the patient is 18 years of age or under.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Measurement" type="MeasurementType" minOccurs="0" maxOccurs="10"/>
			<xsd:element name="ObservationNotes" type="an1..140" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OptionalPharmacy">
		<xsd:annotation>
			<xsd:documentation>Value P2</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Identification" type="PharmacyID" minOccurs="0"/>
			<xsd:element name="Specialty" type="an1..10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value A.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pharmacist" type="Pharmacist" minOccurs="0"/>
			<xsd:element name="BusinessName" type="an1..70"/>
			<xsd:element name="Address" type="AddressType" minOccurs="0"/>
			<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OptionalPrescriber">
		<xsd:annotation>
			<xsd:documentation>Value PC</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="HistoryPrescriber">
				<xsd:sequence>
					<xsd:element name="Identification" type="NonVeterinarianID">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Specialty" type="an1..10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Specialty of prescriber. Health Care Provider Taxonomy Code http://www.wpc-edi.com/reference/</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PracticeLocation" type="PracticeLocationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Name" type="NameType">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="FormerName" type="NameType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Address" type="AddressType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PrescriberAgent" type="NameComposite" minOccurs="0"/>
					<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PrescriberPlaceOfService" type="n2" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifies the place where professional services are rendered resulting in the prescription/order. Required when place of service for the encounter is documented in the patient record.  Otherwise, required when needed to comply with state or federal programs. The CMS Place of Service code set as the ECL values https://www.cms.gov/Medicare/Coding/place-of-service-codes/Place_of_Service_Code_Set.html</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="OptionalPrescriberChoice">
		<xsd:complexContent>
			<xsd:restriction base="HistoryPrescriberChoice">
				<xsd:choice>
					<xsd:element name="NonVeterinarian" type="OptionalPrescriber"/>
					<xsd:element name="Veterinarian" type="VetPrescriberMirrorOptionalPrescriber"/>
				</xsd:choice>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="OrderGroup">
		<xsd:sequence>
			<xsd:element name="OrderGroupNumber" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>Unique ID assigned by the prescriber/pharmacy to link multiple product orders together.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ItemCountInOrderGroup" type="n1..2">
				<xsd:annotation>
					<xsd:documentation>Indicates the specific item counter in order group. i.e. “1”,” 2”,”3” of “1 of 3”, “2 of 3” and “3 of 3”.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TotalCountForOrderGroup" type="n1..2">
				<xsd:annotation>
					<xsd:documentation>Indicates the total number of items within the group. I.e. the “3” of “1 of 3”, “2 of 3” and “3 of 3”.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OrderGroupReason" type="OrderGroupReason">
				<xsd:annotation>
					<xsd:documentation>Indicates the reason why the product orders are being grouped together.  The OrderGroupReason may be the same or may differ with each product within the order group.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OtherMedicationDate">
		<xsd:annotation>
			<xsd:documentation>Applicable Date or Date and Time.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OtherMedicationDate" type="DateType">
				<xsd:annotation>
					<xsd:documentation>Applicable Date or Date and Time</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherMedicationDateQualifier" type="OtherMedicationDateQualifier">
				<xsd:annotation>
					<xsd:documentation>Qualifier indicating the specific date and or date/time represents for the medication prescribed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PAAppeal">
		<xsd:sequence>
			<xsd:element name="IsEAppealSupported" type="BooleanCode">
				<xsd:annotation>
					<xsd:documentation>Indicates if payer supports electronic appeals submission.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExpirationDate" type="DateType" minOccurs="0"/>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PAAppealRequest">
		<xsd:annotation>
			<xsd:documentation>This transaction is a request from the prescriber to the payer to appeal a PA determination.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="PARequestAbstract">
				<xsd:sequence>
					<xsd:element name="Attachment" type="AttachmentWithControlNumber" minOccurs="0"/>
					<xsd:element name="Request" type="PAAppealRequestType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PAAppealRequestType">
		<xsd:sequence>
			<xsd:element name="PACaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AppealCaseID" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific appeal request. If a AppealCaseID is assigned, it is to be exchanged on all subsequent appeal transactions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeadlineForReply" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSet" type="QuestionSetGroupForRequest" minOccurs="0"/>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PAAppealResponse">
		<xsd:annotation>
			<xsd:documentation>This transaction is a response from the payer to the prescriber with the status of a PAAppealRequest.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="PAResponseAbstract">
				<xsd:sequence>
					<xsd:element name="Attachment" type="AttachmentWithControlNumber" minOccurs="0"/>
					<xsd:element name="Response" type="PAResponseStructureForAppeal"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PAApprovedDetail">
		<xsd:sequence>
			<xsd:element name="PACaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationNumber" type="an1..140" minOccurs="0"/>
			<xsd:element name="AuthorizationPeriod" type="DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="Appeal" type="PAAppeal"/>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PAProcessor" type="PAProcessor" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PAApprovedDetailForAppeal">
		<xsd:sequence>
			<xsd:element name="PACaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationNumber" type="an1..140" minOccurs="0"/>
			<xsd:element name="AuthorizationPeriod" type="DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="Appeal" type="PAAppeal"/>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AppealCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific appeal request. If a AppealCaseID is assigned, it is to be exchanged on all subsequent appeal transactions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PAApprovedForCancel">
		<xsd:sequence>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PAAuthorizationDetails">
		<xsd:sequence>
			<xsd:element name="PharmacyType" type="PharmacyType" minOccurs="0"/>
			<xsd:element name="Quantity" type="QuantityAbstract" minOccurs="0"/>
			<xsd:element name="DaysSupply" type="n1..3" minOccurs="0"/>
			<xsd:element name="NumberOfCycles" type="n1..3" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number of cycles authorized.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfRefills" type="n1..2" minOccurs="0"/>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PACancelRequest">
		<xsd:annotation>
			<xsd:documentation>This transaction is a request from the prescriber to the payer to cancel a PARequest.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="PARequestAbstract">
				<xsd:sequence>
					<xsd:element name="Request" type="PACancelRequestStructure"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PACancelRequestStructure">
		<xsd:sequence>
			<xsd:element name="PACaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CancelReasonCode" type="ReasonCode13" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code indicating the reason for the prior authorization cancellation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsPatientNotified" type="BooleanCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the prescriber has notified the patient that the prescriber has requested the PA Request be cancelled.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PACancelResponse">
		<xsd:annotation>
			<xsd:documentation>This transaction is a response from the payer to the prescriber with the status of a PACancelRequest.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RequestReferenceNumber" type="an1..35" minOccurs="0"/>
			<xsd:element name="PAReferenceID" type="an1..35"/>
			<xsd:element name="PAPriorityIndicator" type="PAPriorityIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the requested PA processing priority.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Response" type="PACancelResponseStructure"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PACancelResponseChoiceType">
		<xsd:choice>
			<xsd:element name="Approved" type="PAApprovedForCancel"/>
			<xsd:element name="Denied" type="PADeniedForCancelType"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="PACancelResponseStructure">
		<xsd:sequence>
			<xsd:element name="ReferenceNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Transaction Key. Used in conjunction with same field described above in the RequestReferenceNumber. The Mailbox may return the positional key of the specific piece of mail within their files.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PACaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ResponseStatus" type="PACancelResponseChoiceType"/>
			<xsd:element name="PayerContactCommunicationNumber" type="CommunicationNumbersTypeRestricted" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A number that a prescriber can contact for questions regarding open, closed, approved or denied PAs (this could be different than the question set contact number for open responses)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PACommon" abstract="true">
		<xsd:sequence>
			<xsd:element name="RequestReferenceNumber" type="an1..35" minOccurs="0"/>
			<xsd:element name="PAReferenceID" type="an1..35"/>
			<xsd:element name="PAPriorityIndicator" type="PAPriorityIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the requested PA processing priority.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BenefitsCoordination" type="BenefitsCoordinationRestricted" minOccurs="0" maxOccurs="4">
				<xsd:annotation>
					<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Patient" type="PatientMandatoryAddressType"/>
			<xsd:element name="Pharmacy" type="MandatoryPharmacy" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Prescriber" type="MandatoryPrescriberChoice"/>
			<xsd:element name="MedicationPrescribed" type="PAandREMsPrescribedMedication">
				<xsd:annotation>
					<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Facility" type="Facility" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The Facility composite may be included to describe the care facility or other location at which the patient resides.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Supervisor" type="SupervisorOptionalType" minOccurs="0"/>
			<xsd:element name="Provider" type="Provider" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PADaysSupply">
		<xsd:sequence>
			<xsd:element name="DaysSupply" type="n1..3"/>
			<xsd:element name="DosesPerDay" type="n1..4" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of doses for the prescribed medication the patient is to consume per day.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PADaysSupplyDosePerDay">
		<xsd:sequence>
			<xsd:element name="PADaysSupply" type="PADaysSupply"/>
			<xsd:element name="PADosesPerDay" type="PADosesPerDay"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PADeniedForCancelType">
		<xsd:sequence>
			<xsd:element name="ReasonCode" type="ReasonCode12" minOccurs="0"/>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PADeniedType">
		<xsd:sequence>
			<xsd:element name="PACaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Appeal" type="PAAppeal"/>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PAProcessor" type="PAProcessor" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PADeniedTypeForAppeal">
		<xsd:sequence>
			<xsd:element name="PACaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Appeal" type="PAAppeal"/>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AppealCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific appeal request. If a AppealCaseID is assigned, it is to be exchanged on all subsequent appeal transactions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PADosesPerDay">
		<xsd:sequence>
			<xsd:element name="DaysSupply" type="n1..3" minOccurs="0"/>
			<xsd:element name="DosesPerDay" type="n1..4">
				<xsd:annotation>
					<xsd:documentation>The number of doses for the prescribed medication the patient is to consume per day.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PAInProcessType">
		<xsd:sequence>
			<xsd:element name="PACaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExpectedResponseDate" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The expected date for the resolution of the prior authorization.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PANote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
