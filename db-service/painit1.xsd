	<xsd:element name="AdministrationIndicator" type="AdministrationIndicator">
						<xsd:annotation>
							<xsd:documentation>When structured Sig is as directed per manufacturer's packaging.
Skip Dose.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="AdministrationTiming" type="AdministrationTiming">
						<xsd:annotation>
							<xsd:documentation>Defines a specific administration day, date or time, or event. Examples such as Breakfast, lunch, dinner, sleep, appointment, procedure, surgery, exercise, sex, school, travel, cycle (HRT, PMS, etc.), completion of other medication regimen. Empty stomach.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="AlternateContactRelationship" type="AlternateContactRelationship">
				<xsd:annotation>
					<xsd:documentation>See field 247 – Eligibility/Patient Relationship Code values.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		<xsd:element name="BodyMetricQualifier" type="BodyMetricQualifier" minOccurs="0"/>
				<xsd:element name="ClinicalInformationQualifier" type="ClinicalInformationQualifier"/>
			<xsd:element name="ClinicalSignificanceCode" type="ClinicalSignificanceCode" minOccurs="0"/>
			<xsd:element name="CoAgentCode" type="CoAgentQualifiedCodeWithDescription"/>
		<xsd:element name="Code" type="TransactionErrorCode">
				<xsd:annotation>
					<xsd:documentation>Codes used to relay successful or rejected communications.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Consent" type="Consent" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Note: If Patient Consent = N, it is expected the RxHistory transactions would not be sent.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Diagnosis" type="Diagnosis" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DigestMethod" type="DigestMethod">
						<xsd:annotation>
							<xsd:documentation>Defines the Hashing method, eg. SHA-1 or SHA-256 to be used.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DigitalSignature" type="DigitalSignature" minOccurs="0"/>
			<xsd:element name="DoNotFill" type="DoNotFill" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Dosage" type="Dosage" minOccurs="0"/>
				<xsd:element name="DoseAdministration" type="DoseAdministration"/>
						<xsd:element name="DoseCalculation" type="DoseCalculation" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>This field may often be used in conjunction with the Rate fields within the Timing Segment and/or the Vehicle Segment. Primary use will be in non-retail settings, i.e. hospital, acute care, long term care, home care.  An example using a dose calculation: Cyclophosphamide:  600mg/meter squared in 250ml Normal Saline over 1-2 hours.  Repeat every 2 weeks.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="DrugCoverageStatusCode" type="DrugCoverageStatusCode" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DrugDBCode" type="DrugDBCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>If Compound this field must not be sen</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DrugUseEvaluation" type="DrugUseEvaluation" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Duration" type="Duration"/>
			<xsd:element name="Facility" type="Facility" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The Facility composite may be included to describe the care facility or other location at which the patient resides.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="FacilityID" type="an1..35" minOccurs="0"/>
					<xsd:element name="FinancialGuarantorRelationship" type="FinancialGuarantorRelationship">
				<xsd:annotation>
					<xsd:documentation>Code indicating the relationship between the financial guarantor and the patient.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		<xsd:element name="Gender" type="Gender">
						<xsd:annotation>
							<xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="HospiceIndicator" type="HospiceIndicator" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates patient's hospice status.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				<xsd:element name="Indication" type="SigCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Examples such as Blood Pressure, Finger Stick Blood Glucose</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Instruction" type="Instruction">
					<xsd:annotation>
						<xsd:documentation>Instruction is the focal point of the Structured Sig - An Instruction is defined as Dose Administration with one to many associated Timing and/or Durations</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="InstructionIndicator" type="InstructionIndicator">
					<xsd:annotation>
						<xsd:documentation>When structured Sig is as directed based on medical encounter.
Skip Dose. (Timing is situational.)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="LotNumber" type="LotNumber" minOccurs="0" maxOccurs="9"/>
					<xsd:element name="MandatoryPharmacy" type="MandatoryPharmacy"/>
			<xsd:element name="MaximumDoseRestriction" type="MaximumDoseRestriction" minOccurs="0" maxOccurs="50">
					<xsd:annotation>
						<xsd:documentation>Defines a maximum or dose limit, as specified by the prescriber. The value entered may or may not match with the information provided by clinical decision support tools.  Professional judgment should be used when discrepancies occur.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			<xsd:element name="NeedNoLaterThan" type="NeedNoLaterThan" minOccurs="0" maxOccurs="0"/>
				<xsd:element name="NonHumanPatient" type="NonHumanPatient"/>
		<xsd:element name="OrderCaptureMethod" type="OrderCaptureMethod" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="OrderGroupReason" type="OrderGroupReason">
				<xsd:annotation>
					<xsd:documentation>Indicates the reason why the product orders are being grouped together.  The OrderGroupReason may be the same or may differ with each product within the order group.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		<xsd:element name="OtherMedicationDate" type="OtherMedicationDate" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Applicable Date or Date and Time.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="OtherMedicationDateQualifier" type="OtherMedicationDateQualifier">
				<xsd:annotation>
					<xsd:documentation>Qualifier indicating the specific date and or date/time represents for the medication prescribed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		<xsd:element name="PADaysSupply" type="PADaysSupply"/>
			<xsd:element name="PADaysSupplyDosePerDay" type="PADaysSupplyDosePerDay" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="PADosesPerDay" type="PADosesPerDay"/>
		<xsd:element name="PAInitiationRequest" type="PAInitiationRequest">
				<xsd:annotation>
					<xsd:documentation>This transaction is a request from the prescriber to the payer for the information required to submit a PARequest. It is a request from a prescriber to a payer for the information required to submit a prior authorization request for a specified patient and drug.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PAPriorityIndicator" type="PAPriorityIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the requested PA processing priority.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Patient" type="PatientTypeWithSubstanceUse"/>
					<xsd:element name="PatientLocation" type="PatientLocation" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PayerID" type="an1..80" minOccurs="0" maxOccurs="0"/>
					<xsd:element name="PayerResponsibilityCode" type="PayerResponsibilityCode" minOccurs="0"/>
					<xsd:element name="PayerType" type="PayerType" minOccurs="0"/>
				<xsd:element name="PaymentType" type="PaymentType" minOccurs="0"/>
		<xsd:element name="Pharmacist" type="Pharmacist"/>
			<xsd:element name="Pharmacy" type="MandatoryPharmacy" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Prescriber" type="PrescriberChoice"/>
					<xsd:element name="PriorAuthorizationStatus" type="PriorAuthorizationStatus" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ProductCode" type="ProductCode">
						<xsd:annotation>
							<xsd:documentation>If Compound this field must not be sent.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ProfessionalServiceCode" type="ProfessionalServiceCode" minOccurs="0"/>
			<xsd:element name="Provider" type="Provider" minOccurs="0"/>
		<xsd:element name="Quantity" type="Quantity"/>
					<xsd:element name="RateOfAdministration" type="an1..11"/>
			<xsd:element name="ReasonForSubstitutionCodeUsed" type="ReasonForSubstitutionCodeUsed" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ResidenceCode" type="ResidenceCode" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="RxFillIndicator" type="RxFillIndicator"/>
		<xsd:element name="ServiceReasonCode" type="ServiceReasonCode"/>
			<xsd:element name="ServiceResultCode" type="ServiceResultCode" minOccurs="0"/>
			<xsd:element name="Sig" type="Sig" minOccurs="0"/>
					<xsd:element name="SourceQualifier" type="eclSourceQualifier"/>
		<xsd:element name="SplitScript" type="SplitScript" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation> The prescription has been split for both mail order and retail. The retail order is only for a limited supply  until the mail order arrives.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Strength" type="Strength" minOccurs="0"/>
					<xsd:element name="TimeZone" type="TimeZone" minOccurs="0"/>
		<xsd:element name="TimeZoneIdentifier" type="TimeZoneIdentifier"/>
			<xsd:element name="TimingAndDuration" type="TimingAndDuration"/>
				<xsd:element name="TransferType" type="TransferType"/>
		<xsd:element name="Vehicle" type="Vehicle" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>If the vehicle has a product or ingredient code, the appropriate product code element is used.

The DoseDeliveryMethodModifier provides information as to how the medication is to be delivered via the vehicle, i.e. mixed in, sprinkled on, etc.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			</xsd:schema>
