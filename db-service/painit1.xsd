			<xsd:element name="SequenceNumber" type="n1..3">
				<xsd:annotation>
					<xsd:documentation>Sequence number assigned by the payer to indicate the order of the question in relation to other questions in the question set. Must be unique per question in the transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionLevel" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Question level to indicate grouping of questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionText" type="an1..2000">
				<xsd:annotation>
					<xsd:documentation>Question text to display to the provider.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodedReference" type="CodedReference" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="DefaultNextQuestionID" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A QuestionID or value of "END" to identify the next question when it is not dependent on the answer to the question. Required when the QuestionType is date or free text or when the QuestionType is select or numeric and the NextQuestionID for the answer choices or numeric conditions are not provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="QuestionDetailForRequestType">
		<xsd:complexContent>
			<xsd:extension base="QuestionDetailCommonType">
				<xsd:sequence>
					<xsd:element name="QuestionType" type="QuestionTypeForRequest"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="QuestionDetailForResponseType">
		<xsd:complexContent>
			<xsd:extension base="QuestionDetailCommonType">
				<xsd:sequence>
					<xsd:element name="QuestionType" type="QuestionType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="QuestionSetGroup">
		<xsd:sequence>
			<xsd:element name="Header" type="QuestionSetHeader"/>
			<xsd:element name="Question" type="QuestionDetailForResponseType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="QuestionSetGroupForRequest">
		<xsd:sequence>
			<xsd:element name="Header" type="QuestionSetHeaderWithComment"/>
			<xsd:element name="Question" type="QuestionDetailForRequestType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="QuestionSetGroupForResponse">
		<xsd:sequence>
			<xsd:element name="Header" type="QuestionSetHeader"/>
			<xsd:element name="Question" type="QuestionDetailForResponseType" maxOccurs="unbounded"/>
			<xsd:element name="DeadlineForReply" type="DateType">
				<xsd:annotation>
					<xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="QuestionSetHeader">
		<xsd:sequence>
			<xsd:element name="QuestionSetID" type="an1..70">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the question set. It should not be assumed this is a unique identifier over time or across payers.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSetTitle" type="an1..70">
				<xsd:annotation>
					<xsd:documentation>Title of the question set.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSetDescription" type="an1..2000">
				<xsd:annotation>
					<xsd:documentation>Descriptive information about the question set.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSetContactCommunicationNumber" type="CommunicationNumbersTypeRestricted"/>
			<xsd:element name="AttachementRequired" type="AttachmentRequired">
				<xsd:annotation>
					<xsd:documentation>Used to indicate an attachment is required as part of the criteria.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AttachmentNotes" type="an1..70" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Required if AttachmentRequired value is ‘Yes’. Description of the info required.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="QuestionSetHeaderWithComment">
		<xsd:complexContent>
			<xsd:extension base="QuestionSetHeader">
				<xsd:sequence>
					<xsd:element name="QuestionSetComment" type="an1..2000" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The vendor should always allow the provider to send comments in addition to answering the questions.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="QuestionType">
		<xsd:choice>
			<xsd:element name="Select" type="PASelectType">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is to be selected from a set of choices.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Date" type="DateTypeFlag">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is a date.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Numeric" type="NumericTypeBasic">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is numeric.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FreeText" type="FreeTextTypeBasic">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is FreeText.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="QuestionTypeForRequest">
		<xsd:choice>
			<xsd:element name="Select" type="PASelectTypeForRequest">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is to be selected from a set of choices.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Date" type="DateTypeForRequest">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is a date.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Numeric" type="NumericChoiceForPARequest">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is numeric.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FreeText" type="AnswerForTextType">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is FreeText.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="REMSApprovedDetail">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSAuthorizationNumber" type="an1..140" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationPeriod" type="DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="REMSCommon" abstract="true">
		<xsd:sequence>
			<xsd:element name="RequestReferenceNumber" type="an1..35" minOccurs="0"/>
			<xsd:element name="REMSReferenceID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>Assigned by the prescribing system on the initial transaction and is used as a tracking identifier on all request and response REMS transactions to tieback related REMS transactions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BenefitsCoordination" type="BenefitsCoordinationRestricted" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Patient" type="PatientMandatoryAddressType"/>
			<xsd:element name="Pharmacy" type="MandatoryPharmacy" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Prescriber" type="MandatoryPrescriberChoice"/>
			<xsd:element name="MedicationPrescribed" type="PAandREMsPrescribedMedication">
				<xsd:annotation>
					<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Facility" type="Facility" minOccurs="0"/>
			<xsd:element name="Supervisor" type="SupervisorOptionalType" minOccurs="0"/>
			<xsd:element name="Provider" type="Provider" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="REMSDeniedType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="REMSInitiationRequest">
		<xsd:complexContent>
			<xsd:extension base="REMSRequestAbstract"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="REMSInitiationResponse">
		<xsd:complexContent>
			<xsd:extension base="REMSResponseAbstract">
				<xsd:sequence>
					<xsd:element name="Response" type="REMSResponseStructureRestricted"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="REMSModelType">
		<xsd:sequence>
			<xsd:element name="SolicitedModel" type="REMSSolicitedType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="REMSOpenType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeadlineForReply" type="DateType">
				<xsd:annotation>
					<xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSet" type="QuestionSetGroup" minOccurs="0"/>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="REMSOpenTypeResponse">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MoreInformationRequired" type="QuestionSetGroupForResponse"/>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="REMSReasonClosedCodeType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClosedReasonCode" type="ReasonCode20" maxOccurs="10">
				<xsd:annotation>
					<xsd:documentation>Code indicating the reason for the prior authorization closure.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSAuthorizationNumber" type="an1..140" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationDetail" type="PAAuthorizationDetails" minOccurs="0" maxOccurs="5"/>
			<xsd:element name="AuthorizationPeriod" type="DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="REMSReasonCodeType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonCode" type="ReasonCode20" maxOccurs="10"/>
			<xsd:element name="REMSAuthorizationNumber" type="an1..140" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationDetail" type="PAAuthorizationDetails" minOccurs="0" maxOccurs="5"/>
			<xsd:element name="AuthorizationPeriod" type="DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="REMSRequest">
		<xsd:complexContent>
			<xsd:extension base="REMSRequestAbstract">
				<xsd:sequence>
					<xsd:element name="Attachment" type="AttachmentWithControlNumber" minOccurs="0"/>
					<xsd:element name="Request" type="REMSModelType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="REMSRequestAbstract" abstract="true">
		<xsd:complexContent>
			<xsd:extension base="REMSCommon"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="REMSResponse">
		<xsd:complexContent>
			<xsd:extension base="REMSResponseAbstract">
				<xsd:sequence>
					<xsd:element name="Attachment" type="AttachmentWithControlNumber" minOccurs="0"/>
					<xsd:element name="Response" type="REMSResponseStructureCommon"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="REMSResponseAbstract" abstract="true">
		<xsd:complexContent>
			<xsd:extension base="REMSCommon"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="REMSResponseChoiceType">
		<xsd:choice>
			<xsd:element name="Approved" type="REMSApprovedDetail">
				<xsd:annotation>
					<xsd:documentation>Either AuthorizationDetail or AuthorizationPeriod must be sent.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Denied" type="REMSDeniedType"/>
			<xsd:element name="Open" type="REMSOpenTypeResponse"/>
			<xsd:element name="Closed" type="REMSReasonClosedCodeType"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="REMSResponseChoiceTypeRestricted">
		<xsd:choice>
			<xsd:element name="Open" type="REMSOpenType"/>
			<xsd:element name="Closed" type="REMSReasonCodeType"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="REMSResponseStructureCommon">
		<xsd:sequence>
			<xsd:element name="ReferenceNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Transaction Key. Used in conjunction with same field described above in the RequestReferenceNumber. The Mailbox may return the positional key of the specific piece of mail within their files.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ResponseStatus" type="REMSResponseChoiceType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="REMSResponseStructureRestricted">
		<xsd:sequence>
			<xsd:element name="ReferenceNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Transaction Key. Used in conjunction with same field described above in the RequestReferenceNumber. The Mailbox may return the positional key of the specific piece of mail within their files.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ResponseStatus" type="REMSResponseChoiceTypeRestricted"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="REMSSolicitedType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeadlineForReply" type="DateType">
				<xsd:annotation>
					<xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSet" type="QuestionSetGroupForRequest" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RateOfAdministration">
		<xsd:sequence>
			<xsd:element name="RateOfAdministration" type="an1..11"/>
			<xsd:element name="RateUnitOfMeasure" type="SigCode">
				<xsd:annotation>
					<xsd:documentation>Examples such as seconds, minutes, hours, days.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TimePeriodBasis" type="SigCode">
				<xsd:annotation>
					<xsd:documentation>Examples such as minute, hour, day. For example, a dosing basis unit of measure of mg/kg to be calculated for a 24 hour time period would have a final representation of mg/kg/day.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Recertification">
		<xsd:annotation>
			<xsd:documentation>This transaction is used to send a renewal request from a facility to a pharmacy.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="SCRIPTRequest">
				<xsd:sequence>
					<xsd:element name="MessageRequestCode" type="MessageFunctionCode" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ReturnReceipt" type="an1..3" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="RequestReferenceNumber" type="an1..35" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="FollowUpRequest" type="n1..1" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ChangeOfPrescriptionStatusFlag" type="ChangeOfPrescriptionStatusCode" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="AllergyOrAdverseEvent" type="AllergyChoice" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The AllergyOrAdverseEvent element enables the sender to inform the recipient of all known patient allergies. The element is intended to provide this patient Allergy or AdverseEvent profile information to the recipient. If the AllergyOrAdverseEvent element is sent, it must include all known patient allergies. Responder can send back an AllergyOrAdverseEvent element, but should not echo back the AllergyOrAdverseEvent element received on the request. Note: The AllergyOrAdverseEvent element is not to be used to replace or add to the transfer of drug use evaluation information, which is instead communicated using the existing DUE composite in the medication information. This is a comprehensive list of allergies that the sender is aware of at the time the transaction is sent. Trading partners must support the AllergyOrAdverseEvent Element but use is optional on a case by case basis.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="BenefitsCoordination" type="BenefitsCoordination" minOccurs="0" maxOccurs="4">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DiagnosisGeneral" type="DiagnosisGeneral" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Facility" type="Facility" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The Facility composite may be included to describe the care facility or other location at which the patient resides.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="FillStatus" type="FillStatus" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Patient" type="PatientTypeWithSubstanceUse"/>
					<xsd:element name="Pharmacy" type="MandatoryAddressPharmacy">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Prescriber" type="PrescriberChoice"/>
					<xsd:element name="DrugAdministrationRequest" type="DrugAdministrationRequest" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Observation" type="Observation" minOccurs="0"/>
					<xsd:element name="MedicationDispensed" type="RecertificationMedicationForDispensed" minOccurs="0"/>
					<xsd:element name="MedicationPrescribed" type="RecertificationMedication"/>
					<xsd:element name="MedicationRequested" type="ResupplyMedication" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Supervisor" type="SupervisorType" minOccurs="0"/>
					<xsd:element name="RequestedDates" type="DateRange" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See SCRIPTRequest of Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ProhibitRenewalRequest" type="xsd:boolean" minOccurs="0" maxOccurs="0"/>
					<xsd:element name="ChangeReasonText" type="an1..260" minOccurs="0" maxOccurs="0"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="RecertificationMedication">
		<xsd:complexContent>
			<xsd:restriction base="MedicationAbstract">
				<xsd:sequence>
