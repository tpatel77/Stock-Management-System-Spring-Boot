						<xsd:documentation>value RA</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="CancelSuspension" type="xsd:anySimpleType">
					<xsd:annotation>
						<xsd:documentation>value CS</xsd:documentation>
