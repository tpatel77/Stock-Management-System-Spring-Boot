						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Specialty" type="an1..10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Pharmacist" type="Pharmacist" minOccurs="0"/>
					<xsd:element name="BusinessName" type="an1..70">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Address" type="MandatoryAddressType">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
