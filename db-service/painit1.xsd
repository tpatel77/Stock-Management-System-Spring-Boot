							<xsd:documentation>See SpecialzedResponse for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ClinicalInfoTypesRequested" type="ClinicalInfoTypesRequested" minOccurs="0" maxOccurs="0">
