					<xsd:element name="IINNumber" type="an1..35" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Card Issuer ID or Issuer Identification Number used for network routing in the following datatypes.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
